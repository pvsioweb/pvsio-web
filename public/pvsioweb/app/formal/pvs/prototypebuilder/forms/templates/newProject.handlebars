<form class="center dialog shadow form-horizontal" onsubmit="return false" role="form">
	<legend>New Project</legend>
	<div class="form-group">
		<label for="projectName" class="col-sm-4 control-label">Project Name</label>
		<div class="col-sm-8">
			<input id="projectName" class="form-control" type="text" value="" name="projectName" required="true"/>
		</div>
	</div>
	<div class="form-group">
		<label class="control-label col-sm-4" for="prototypeImage">Prototype Image</label>
		<div class="col-sm-8">
			<input id="prototypeImage" class="form-control" type="file" value="" name="prototypeImage" required="true">
		</div>
	</div>
	<div class="form-group">
		<label class="control-label col-sm-4" for="pvsSpec">PVS Spec</label>
		<div class="col-sm-8">
			<input id="pvsSpec" class="form-control" type="file" value="" name="pvsSpec" required="true" multiple="true">
		</div>
	</div>
	<div class="form-group">
		<div class="col-sm-offset-4 col-sm-8">
			<button type="button" class="btn btn-danger left" id="btnCancel">Cancel</button>
			<button type="submit" class="btn btn-success right" id="btnOk">OK</button>
		</div>

	</div>
</form>