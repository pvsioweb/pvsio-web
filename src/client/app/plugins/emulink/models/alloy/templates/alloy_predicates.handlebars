{{#if template_description}}
%
%   Description: This template generates alloy predicates corresponding to emucharts transitions between states (modes) of the system.
%{{/if}}{{#if comment}}/**
 * {{comment}}
 */{{/if}}{{#each functions}}
pred {{name}}[st1, st2: State] {
  {{#each cases}}({{cond}}) implies
     st2.{{../../mode_attribute}} := {{to}}{{#each actions}}
     st2.{{variable_name}} := {{value}}{{/each}}
  else {{/each}}st1 = st2
}{{/each}}
